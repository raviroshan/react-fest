{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": 0,
    "max-len": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,

    // React Rules
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/forbid-prop-types": 0,
    "react/prop-types": 0,
    "react/no-unused-prop-types": 0,

    "jsx-a11y/anchor-is-valid": 0
  }
}
